{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5004"
      },
      "Https": {
        "Url": "https://*:7004",
        "Protocols": "Http1AndHttp2",
        "SslProtocols": "Tls12",
        "Certificate": {
          "Path": "./certificate.pfx",
          "Password": "your_certificate_password"
        }
      }
    }
  }
}
